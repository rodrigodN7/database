USE firstdb;
CREATE TABLE sales_rep(

	employee_number INT,
	surname VARCHAR(40),
	first_name VARCHAR(30),
	comission TINYINT

);

#Mostrar tablas existentes en la base datos firstdb
SHOW TABLES;

#Show a description of the created table
DESCRIBE sales_rep;
